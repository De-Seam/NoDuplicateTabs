{
  "manifest_version": 3,
  "name": "NoDuplicateTabs",
  "version": "1.1",
  "description": "Removes duplicate tabs",
  "background": {
    "service_worker": "NoDuplicateTabs.js"
  },
  "permissions": [
    "tabs",
    "storage"
  ],
  "host_permissions": [
    "*://*/*"
  ],
  "web_accessible_resources": [
    {
      "resources": ["UrlsToClose.txt"],
      "matches": ["*://*/*"]
    }
  ]
}
