{
  "manifest_version": 2,
  "name": "NoDuplicateTabs",
  "version": "1.1",
  "description": "Removes duplicate tabs",
  "background": {
    "scripts": ["NoDuplicateTabs.js"],
    "persistent": false
  },
  "permissions": [
    "tabs"
  ],
  "applications": {
    "gecko": {
      "id": "no-duplicate-tabs@mydomain.com"
    }
  }
}
